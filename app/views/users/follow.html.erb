<div class ="container">
  <div class ="row">
    <div class ="col-lg-12 text-center">
      <h2>Following users</h2>
      <table class="table">
        <thead>
         <tr>
          <th>
           </h1>Username</h1>
          </th>
           </tr>
               <% @user.followings.each do |user| %>
            <tr>
                <td><%= link_to user_path(user) do  %><%= user.name  %><% end %></td>
             </tr>
               <% end %>
      </table>
    </div>
  </div>
</div>